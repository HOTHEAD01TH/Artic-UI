{
  "name": "artic-ui",
  "version": "0.0.6",
  "description": "A modern React component library with beautiful, accessible components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react",
    "dev": "tsup src/index.tsx --format cjs,esm --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rimraf .turbo node_modules dist"
  },
  "devDependencies": {
    "@swc/core": "^1.9.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^8.55.0",
    "eslint-config-custom": "*",
    "react": "^19.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^3.0.2"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.0",
    "framer-motion": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "tailwindcss": "^3.0.0 || ^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwindcss",
    "framer-motion"
  ]
}
